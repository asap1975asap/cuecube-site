<% layout('layout') %>
<div class="form-wrap">
    <h2>Dealer Login</h2>
    <% if (error) { %>
        <p class="error"><%= error %></p>
    <% } %>
    <form method="post" action="/login">
        <label>Email
            <input type="email" name="email" value="demo@cuecube.com" required />
        </label>
        <label>Password
            <input type="password" name="password" required />
        </label>
        <button type="submit" class="btn">Login</button>
    </form>
</div>
